{
    "name": "lostpets-web",
    "version": "1.0.0",
    "license": "MIT",
    "author": "Robert Stefanita Ene",
    "description": "Web client for LostPets system.",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "config": "ts-node -O '{\"module\": \"commonjs\"}' ./config.index.ts",
        "build": "npm run config && ng build --prod --base-href / --aot=false --build-optimizer=false",
        "build:docs": "typedoc --out doc src --ignoreCompilerErrors",
        "build:aws": "npm run config && ng build --prod --configuration=aws --base-href / --aot=false --build-optimizer=false",
        "build:heroku": "npm run config && ng build --prod --configuration=heroku --base-href / --aot=false --build-optimizer=false",
        "build:ci": "npm run config && ng build --prod --configuration=heroku --base-href / --aot=false --build-optimizer=false",
        "test": "ng test --no-watch --code-coverage",
        "test:ci": "ng test --configuration=ci --watch=false --code-coverage --source-map true",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "e2e:ci": "ng e2e --configuration=ci",
        "postinstall": "webdriver-manager update --standalone false --gecko false --versions.chrome 76.0.3809.68"
    },
    "private": true,
    "dependencies": {
        "@agm/core": "1.1.0",
        "@angular/animations": "~8.2.14",
        "@angular/cdk": "8.2.3",
        "@angular/common": "~8.2.14",
        "@angular/compiler": "~8.2.14",
        "@angular/core": "~8.2.14",
        "@angular/forms": "~8.2.14",
        "@angular/platform-browser": "~8.2.14",
        "@angular/platform-browser-dynamic": "~8.2.14",
        "@angular/router": "~8.2.14",
        "@auth0/angular-jwt": "4.0.0",
        "@types/crypto-js": "3.1.44",
        "@types/stompjs": "2.3.4",
        "core-js": "3.3.3",
        "crypto-js": "4.0.0",
        "jw-angular-pagination": "1.1.0",
        "net": "1.0.2",
        "primeicons": "2.0.0",
        "primeng": "8.1.1",
        "process": "0.11.10",
        "rxjs": "~6.4.0",
        "stompjs": "2.3.3",
        "tslib": "^1.10.0",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.803.25",
        "@angular/cli": "~8.3.26",
        "@angular/compiler-cli": "~8.2.14",
        "@angular/language-service": "~8.2.14",
        "@ngneat/spectator": "5.6.1",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "codelyzer": "^5.0.0",
        "jasmine-core": "3.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.1.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "1.5.3",
        "protractor": "~5.4.0",
        "puppeteer": "chrome-76",
        "ts-node": "~7.0.0",
        "tslint": "~5.15.0",
        "typedoc": "0.17.4",
        "typescript": "~3.5.3"
    }
}
